  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch server",
      "program": "${workspaceFolder}/server/index.js",
      "cwd": "${workspaceFolder}/server"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID - Nodemon",
      "processId": "${command:PickProcess}",
      "restart": true
    }
  ]
}
